{
  "expo": {
    "name": "Xito",
    "slug": "Xito",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/Logo.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/Logo.png"   
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xito.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ],
      "package": "com.xito.app"
    },
    "extra": {
      "eas": {
        "projectId": "96689133-fca2-4b3a-acc5-07bd1b5a2949"
      }
    }
  }
}
